    "components": {
        "schemas": {
            "CreateCategoryRequest": {
                "required": [
                    "name"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 255,
                        "example": "Electronics"
                    }
                },
                "type": "object"
            },
            "CreateEntryRequest": {
                "required": [
                    "product_id",
                    "quantity"
                ],
                "properties": {
                    "product_id": {
                        "type": "string",
                        "example": "1"
                    },
                    "quantity": {
                        "type": "integer",
                        "example": 50
                    }
                },
                "type": "object"
            },
            "CreateOutputRequest": {
                "required": [
                    "product_id",
                    "quantity"
                ],
                "properties": {
                    "product_id": {
                        "type": "string",
                        "example": "1"
                    },
                    "quantity": {
                        "type": "integer",
                        "example": 20
                    }
                },
                "type": "object"
            },
            "CreateProductRequest": {
                "required": [
                    "name",
                    "category_id",
                    "price",
                    "stock"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Laptop"
                    },
                    "category_id": {
                        "type": "string",
                        "example": "1"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 1500
                    },
                    "stock": {
                        "type": "integer",
                        "example": 100
                    }
                },
                "type": "object"
            },
            "UpdateProductRequest": {
                "required": [
                    "name",
                    "price",
                    "stock"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Updated Laptop"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 1600
                    },
                    "stock": {
                        "type": "integer",
                        "example": 120
                    }
                },
                "type": "object"
            },
            "Category": {
                "title": "Category",
                "description": "Category model",
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "1234"
                    },
                    "name": {
                        "type": "string",
                        "example": "Eletrônicos"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "Entry": {
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "1"
                    },
