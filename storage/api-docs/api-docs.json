                    "stock": {
                        "type": "integer",
                        "example": 100
                    }
                },
                "type": "object"
            },
            "UpdateProductRequest": {
                "required": [
                    "name",
                    "price",
                    "stock"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "example": "Updated Laptop"
                    },
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": 1600
                    },
                    "stock": {
                        "type": "integer",
                        "example": 120
                    }
                },
                "type": "object"
            },
            "Category": {
                "title": "Category",
                "description": "Category model",
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "1234"
                    },
                    "name": {
                        "type": "string",
                        "example": "Eletrônicos"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "Entry": {
